# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           		controllers.Application.index
GET		/taskProcess/:task/:path			controllers.Application.generateTaskProcess(task: String, path: String)
POST	/generateSchedulePdf				controllers.Application.generateSchedulePdf
GET		/personalGraph/:link				controllers.Application.generatePersonalGraph(link: String)
POST	/generateLink						controllers.Application.generateLink
POST	/getGraphData/:link					controllers.Application.getGraphData(link: String)



# Map static resources from the /public folder to the /assets URL path
GET     /*file               				controllers.Assets.at(path="/public", file)

